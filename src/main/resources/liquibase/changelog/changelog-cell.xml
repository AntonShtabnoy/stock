<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
                   http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <changeSet id="13.11.2017_22:45" author="rijikpijik212011@gmail.com">
        <createTable tableName="cell">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="storage_id" type="bigint">
                <constraints foreignKeyName="storage_id_fk" referencedTableName="storage" referencedColumnNames="id"/>
            </column>
            <column name="type" type="varchar(255)"/>
            <column name="status" type="varchar(20)"/>
            <column name="number" type="varchar(255)"/>
            <column name="goods_id" type="bigint">
                <constraints foreignKeyName="goods_id_fk" referencedTableName="goods" referencedColumnNames="id"/>
            </column>
            <column name="count" type="int"/>
        </createTable>
    </changeSet>
</databaseChangeLog>